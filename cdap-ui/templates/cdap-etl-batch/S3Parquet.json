{
  "id": "S3Parquet",
  "groups" : {
    "position": [ "group1" ],
    "group1": {
      "display" : "S3 Sink - Parquet",
      "position" : [ "accessID", "accessKey", "basePath", "pathFormat", "fileSystemProperties" ],
      "fields" : {
        "accessID" : {
          "widget": "textbox",
          "label": "Access ID"
        },

        "accessKey" : {
          "widget": "textbox",
          "label": "Access Key"
        },

        "basePath" : {
          "widget": "textbox",
          "label": "Path"
        },

        "pathFormat" : {
          "widget": "textbox",
          "label": "Path Format"
        },

        "fileSystemProperties" : {
          "widget": "json-editor",
          "label": "File System Properties"
        }
      }
    }
  },
  "schema" : {
     "widget": "schema",
     "schema-types" : [ "boolean", "int", "long", "float", "double", "bytes", "string", "map<string, string>" ],
     "schema-default-type" : "string"
  }

}
