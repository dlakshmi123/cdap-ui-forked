{
  "id": "SnapshotParquet",
  "groups" : {
    "position": [ "group1" ],
    "group1": {
      "display" : "Snapshot Fileset - Parquet",
      "position" : [ "name", "basePath" ],
      "fields" : {
        "name" : {
          "widget": "dataset-selector",
          "label": "Dataset Name"
        },

        "basePath" : {
          "widget": "textbox",
          "label": "Snapshot Target Path"
        }
      }
    }
  },
  "schema" : {
    "widget": "schema",
    "schema-types" : [ "boolean", "int", "long", "float", "double", "bytes", "string", "map<string, string>" ],
    "schema-default-type" : "string"
  }
}
