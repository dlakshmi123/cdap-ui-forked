#!/usr/bin/env bash

bin=`dirname "${BASH_SOURCE-$0}"`
bin=`cd "$bin"; pwd`
script=`basename $0`

function usage() {
  echo "Usage: $script [--host <host>]"
  echo ""
  echo "  Options"
  echo "    --host      Specifies the host that Reactor is running on. (Default: localhost)"
  echo "    --help      This help message"
  echo ""
}

gateway="localhost"
while [ $# -gt 0 ]
do
  case "$1" in 
    --host) shift; gateway="$1"; shift;;
    *)  usage; exit 1
  esac
done

pushd $bin 2>/dev/null >/dev/null
./generateRandomOrderData.sh -h $gateway -p 10000
popd 2>/dev/null >/dev/null
